generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" // You can change this to "postgresql" later
  url      = "file:./dev.db"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  password  String?
  createdAt DateTime @default(now())
}


